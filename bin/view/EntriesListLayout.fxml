<AnchorPane prefHeight="600.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EntriesListController">
   <children>
      <ListView fx:id="entriesList" layoutX="20.0" layoutY="141.0" prefHeight="445.0" prefWidth="537.0" />
    <Button layoutX="428.0" layoutY="52.0" mnemonicParsing="false" onAction="#deleteButton" prefHeight="31.0" prefWidth="106.0" text="Delete" />
      <Button layoutX="428.0" layoutY="11.0" mnemonicParsing="false" onAction="#addEntry" prefHeight="31.0" prefWidth="106.0" text="Add" />
      <Separator layoutY="126.0" prefHeight="15.0" prefWidth="570.0" />
      <TextField fx:id="usernameField" layoutX="157.0" layoutY="14.0" prefHeight="31.0" prefWidth="263.0" />
      <TextField fx:id="passwordField" layoutX="157.0" layoutY="47.0" prefHeight="31.0" prefWidth="263.0" />
      <TextField fx:id="descriptionField" layoutX="157.0" layoutY="87.0" prefHeight="31.0" prefWidth="263.0" />
      <Label layoutX="47.0" layoutY="18.0" text="Username :">
         <font>
            <Font name="MS Gothic" size="20.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="52.0" text="Password :">
         <font>
            <Font name="MS Gothic" size="20.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="91.0" prefHeight="21.0" prefWidth="133.0" text="Description :">
         <font>
            <Font name="MS Gothic" size="20.0" />
         </font>
      </Label>
      <Button layoutX="428.0" layoutY="94.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="31.0" prefWidth="106.0" text="Save&amp;Close" />
   </children>
</AnchorPane>
