<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SignUpController">
   <children>
      <Label layoutX="91.0" layoutY="7.0" text="Sign Up">
         <font>
            <Font name="MS Gothic" size="30.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="56.0" text="Choose username">
         <font>
            <Font name="MS Gothic" size="24.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="138.0" text="Choose password">
         <font>
            <Font name="MS Gothic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="usernameCreate" layoutX="41.0" layoutY="93.0" prefHeight="31.0" prefWidth="220.0" />
      <PasswordField fx:id="passwordCreate" layoutX="41.0" layoutY="178.0" prefHeight="31.0" prefWidth="220.0" />
      <Button layoutX="152.0" layoutY="213.0" mnemonicParsing="false" onAction="#okButton" prefHeight="40.0" prefWidth="110.0" text="OK">
         <font>
            <Font name="MS Gothic" size="23.0" />
         </font>
      </Button>
      <Button layoutX="40.0" layoutY="213.0" mnemonicParsing="false" onAction="#cancelButton" prefHeight="38.0" prefWidth="110.0" text="Cancel">
         <font>
            <Font name="MS Gothic" size="23.0" />
         </font>
      </Button>
      <Label fx:id="creationStatus" layoutX="133.0" layoutY="262.0" />
   </children>
</AnchorPane>
